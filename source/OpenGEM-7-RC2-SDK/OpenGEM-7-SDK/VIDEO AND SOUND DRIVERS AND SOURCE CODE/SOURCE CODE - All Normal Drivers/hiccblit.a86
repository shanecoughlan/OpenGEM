screen_aligned_blit:
	mov	bl, num_colors - 1
	call	box_plane_enable	;Set all planes to draw

	mov	bx, PTSIN
	mov	ax, PTSIN +4		;get x1, x2
	mov	dx, ax
	sub	dx, bx			;get the delta
	inc	dx
	mov	si, bx
	and	si, 7			;find the left mask
	mov	cl, byte_mask[si]	;cl = left mask
	sub	si, 8
	neg	si			;si = left count
	mov	di, ax
	inc	di
	and	di, 7
	mov	ch, byte_mask[di]
	not	ch
;	and	ax, 7			;ch = right mask ax = right count
	cmp	dx, 8			;is this a single byte out
	jnl	screen_aligned_long
	and	cl, ch			;left mask = left & right
	xor	dx, dx
	xor	ch, ch
	jmps	screen_aligned_mask_done

screen_aligned_long:
	sub	dx, si			;middle count = (delta - left -right)/8
;	sub	dx, ax
	sub	dx, di
	shr	dx,1
	shr	dx,1
	shr	dx,1
	cmp	si, 8
	jnz	screen_aligned_mask_done
	xor	cl,cl
	inc	dx

screen_aligned_mask_done:
	mov	dest_words, dx		;this is the middle count
	mov	bx, PTSIN		;bx = sx1
	mov	dx, PTSIN +8		;dx = dx1
	mov	si, 1
	cmp	backwards, FALSE
	jz	screen_aligned_notback
	xchg	ch,cl
	neg	si			;change the direction flag
	std				;make it autodecrement
	mov	bx, PTSIN +4		;bx = sx2
	mov	dx, PTSIN +12
screen_aligned_notback:
	mov	dest_next, si
	push	cx
	mov	ax, PTSIN +2		;ax = sy1
	mov	cx, PTSIN +10		;cx = dy1
	mov	source_next, next_line	;default is move upward
	mov	swrap, true_to_first
	cmp	yover, FALSE
	jz	screen_aligned_notrev
	mov	ax, PTSIN +6
	mov	cx, PTSIN +14
;;;	neg	source_next
	mov	source_next, neg_next_line
	mov	swrap, true_to_last
screen_aligned_notrev:
	push	dx
	push	cx
	call	concat			;find the source pointer
	mov	si, di
	pop	ax
	pop	bx
	push	si
	call	concat			;find the dest pointer
	pop	si
	pop	bx			;get the masks back
	mov	dx, graph_plane
	mov	es, dx			;init the pointer to the screen
	mov	cx, dest_words		;use this as the count
	mov	dx, crtc_sel_plane_port
	mov	ax, 6F19h		;Put the controller in AND mode
	out	dx, ax
screen_aligned_loop:
	push	ds			;save the data seg
	push	es
	pop	ds			;make the segs point at screen
	push	di
	push	si
	push	cx			;save the count of words
	lodsb				;Read source latches
	mov	ah, bl			;bl = left fringe
	mov	al, 1Bh
	out	dx, ax			;Protect bits that will be written.
	mov	al, es:[di]		;Load unprotected bits
	mov	al, 0FFh	
	stosb				;Store 0FFh to copy the bits.
	mov	ax, 001Bh
	out	dx, ax			;And unprotect the latches again.
	pop	cx
	push	cx
	jcxz	screen_aligned_right
;
; Move the middle portion: CX bytes from [si] to [di]
;
scr_align_copy:
	lodsb				;Read source latches
	mov	al, 0FFh		;Write 0FFh. This will store the
	stosb				;value in the latches.
	loop	scr_align_copy
screen_aligned_right:
	lodsb				;Load the last source byte.
	mov	ah, bh			;bh = right fringe
	mov	al, 1Bh
	out	dx, ax			;Protect necessary bits.
	mov	al, es:[di]		;Load the latches from the dest byte.
	mov	al, 0FFh
	stosb				;Copy the bytes	
	mov	ax, 001Bh		;Unprotect the latches ready for
	out	dx, ax			;the next byte
	pop	cx			;get the count off the stack
	pop	si
	pop	di
	pop	ds			;get the data seg back
	add	si, source_next		;get to next line
	cmp	si, true_plane_size
	jc	sar_1
	add	si, swrap
sar_1:	add	di, source_next		;goto the next scan line
	cmp	di, true_plane_size
	jc	sar_2
	add	di, swrap
sar_2:	dec	height
	jnz	screen_aligned_loop
	cld
	call	box_hic_exit		;reset registers for next time
	jmp	blt_done
;
tab_init_tbl	dw	offset tab_init_replace
		dw	offset tab_init_tran
		dw	offset tab_init_xor
		dw	offset tab_init_itran

tran_blit_loop	dw	offset tran_blit_replace
		dw	offset tran_blit_tran
		dw	offset tran_blit_xor
		dw	offset tran_blit_itran

tran_aligned_blit:
;
; We have 4 cases: Replace, Tran, Xor, Invtran
;
	mov	bl, num_colors - 1
	call	box_plane_enable
	mov	bx, INTIN	;Write mode
	dec	bx
	and	bx, 3		;0-3
	add	bx, bx
	call	cs:tab_init_tbl[bx]
	call	box_hic_exit
	jmp	blt_done
;
tab_init_replace:
	mov	bx, intin+2	;Foreground
	shl	bx, 1
	mov	al, byte ptr MAP_COL[bx]
	mov	bx, intin+4	;Background
	shl	bx, 1
	mov	ah, byte ptr MAP_COL[bx]
	and	ax, 0F0Fh
	mov	cl, 4
	shl	ah, cl
	or	al, ah		;AL = foreground / background
	mov	bl, al
	mov	bh, 4Fh		;Write mode 0
	call	box_hic_init
	jmp	tran_aligned_blit_ok
;
tab_init_tran:
	mov	bx, intin+2	;Foreground
	shl	bx, 1
	mov	ax, MAP_COL[bx]
	mov	ah, al
	and	ax, 0F0Fh
	mov	cl, 4
	shl	ah, cl
	or	al, ah		;AL = foreground / background
	mov	bl, al
	mov	bh, 10h		;Write mode 1 (or)
	call	box_hic_init
	jmp	tran_aligned_blit_ok
;
tab_init_xor:
	mov	bx, 3F0Fh	;Write mode 3 (xor)
	call	box_hic_init
	jmp	tran_aligned_blit_ok
;
tab_init_itran:
	mov	bx, intin+4	;Background
	shl	bx, 1
	mov	ax, MAP_COL[bx]
	mov	ah, al
	and	ax, 0F0Fh
	mov	cl, 4
	shl	ah, cl
	or	al, ah		;AL = foreground / background
	mov	bl, al
	mov	bh, 60h		;Write mode 2 (and)
	call	box_hic_init	;FALL THROUGH to...

tran_aligned_blit_ok:
	mov	bx, PTSIN+8
	mov	ax, PTSIN+10
	call	concat			;di = the dest pointer
	mov	ax, graph_plane
	mov	es, ax
	mov	bp, height
	mov	cx, width
	shr	cx, 1
	shr	cx, 1
	shr	cx, 1
	mov	ax, source_next
	mov	si, PTSIN+2
	mul	si
	mov	dx, source_next
	mov	si, PTSIN
	shr	si, 1
	shr	si, 1
	shr	si, 1
	add	si,ax
	add	si, s_off
	mov	bx, INTIN	;BX = write mode, 1-4
	dec	bx		;0-3
	and	bx, 3
	add	bx, bx
	push	s_seg
	pop	ds
	jmp	cs:tran_blit_loop[bx]
;
;ds:si = source pointer data
;es:di = dest pointer
;cx = inner loop count
;bp = height
;bx = next screen line
;dx = next source line
;
tran_blit_replace:
	push	cx
	push	si		;For a byte-aligned write of foreground and
	push	di		;background, the InColor makes things very
	rep	movsb		;easy for us.
	pop	di
	pop	si
	pop	cx
	add	si, dx
	add	di, next_line
	cmp	di, true_plane_size
	jc	tran_blit_replace_l_1
	add	di, true_to_first
tran_blit_replace_l_1:
	dec	bp
	jnz	tran_blit_replace
	ret

tran_blit_tran:
	push	cx
	push	si
	push	di
tran_blit_tran_loop:
	lodsb				;get the source byte 
	or	es:[di], al		;OR onto the destination byte
	inc	di
	loop	tran_blit_tran_loop
	pop	di
	pop	si
	pop	cx
	add	si, dx
	add	di, next_line
	cmp	di, true_plane_size
	jc	tran_blit_tran_l_1
	add	di, true_to_first
tran_blit_tran_l_1:
	dec	bp
	jnz	tran_blit_tran
	ret
;
tran_blit_xor:
	push	cx
	push	si
	push	di
tran_blit_xor_loop:
	lodsb				;get the source byte 
	xor	es:[di], al		;XOR onto the destination byte
	inc	di
	loop	tran_blit_xor_loop
	pop	di
	pop	si
	pop	cx
	add	si, dx
	add	di, next_line
	cmp	di, true_plane_size
	jc	tran_blit_xor_l_1
	add	di, true_to_first
tran_blit_xor_l_1:
	dec	bp
	jnz	tran_blit_xor
	ret
;
tran_blit_itran:
	push	cx
	push	si
	push	di
tran_blit_itran_loop:
	lodsb				;get the source byte 
	and	es:[di], al		;AND onto the destination byte
	inc	di
	loop	tran_blit_itran_loop
	pop	di
	pop	si
	pop	cx
	add	si, dx
	add	di, next_line
	cmp	di, true_plane_size
	jc	tran_blit_itran_l_1
	add	di, true_to_first
tran_blit_itran_l_1:
	dec	bp
	jnz	tran_blit_itran
	ret




	endif	;not GSX
