;****************************************	
;escape func 2
; Enter Graphics Mode 
;****************************************	
INIT_G:
	pushf
	mov	ax,cs
	mov	usermot+2,ax		;init the user call vectors segs
	mov	userbut+2,ax
	mov	usercur+2,ax
	mov	ax,offset umotvec
	mov	usermot,ax
	mov	ax,offset ubutvec
	mov	userbut,ax
	mov	ax,offset mov_cur
	mov	usercur,ax
	popf
escfn2: 
	cmp	dispmode,0
	jnz	escfn2_ok
	ret
escfn2_ok:
	mov	dispmode,0		; 

  	push	ds
 	mov	ax,0
  	mov	ds,ax
 	mov	bx,410h
 	mov	dl,[bx] 		;get current screen adapter
  	mov	al,dl
 	and	al,0cfh
  	or	al,10h
  	mov	[bx],al 		;select color card
  	pop	ds
	mov	mode_save,dl
	mov	ax, 0F00h
	int	10h
	mov	mode_save2, al
	mov	ax, 79h			;Genoa 6400 800x600 mode
  	int	screen
	cmp	al, 0FFh
	jnz	gotmode
	mov	dl, mode_save		;Mode set failed. Restore previous.
	push	ds
	xor	ax, ax
	mov	ds, ax
	mov	bx, 410h
	mov	[bx], dl
	pop	ds
	mov	al, mode_save2
	int	10h			;Set previous mode
	mov	ah, 2
	xor	dx, dx
	mov	bh, dh
	int	10h			;Home the cursor
	mov	ax, 920h
	mov	bx, 7
	mov	cx, 2000
	int	10h			;Clear screen
	mov	ah, 9
	mov	dx, offset aieee
	int	21h
	mov	ax, 4C00h
	int	21h

gotmode:
	call	init_lut
if num_planes eq 2			;is this a four color driver
	mov	dx, 3ceh		;init the misc reg
	mov	al, 6
	out	dx, al
	inc	dx
	mov	al, 00000111b		;allow odd/even ,graphics, 64k ram
	out	dx, al
	mov	dx, 3ceh		;init the mode reg
	mov	al, 5
	out	dx, al
	inc	dx
	mov	al, 00010000b 		;allow odd/even
	out	dx, al
	mov	dx, 3c4h		;allow odd/even, no extended memory
	mov	al, 4
	out	dx, al
	inc	dx
	mov	al, 0
	out	dx, al
endif	
	mov	dx, 3c4h
	mov	al, 02
	out	dx, al			; init the plane mask add reg
	mov	dx, 3ceh
	mov	al, 04
	out	dx, al
	call	clearmem		; clear graphics display
if mouse
	mov	bl,1
	call	mouse_function		;initialize the mouse
	mov	bl,2
	call	mouse_function
	mov	bl,1
	call	mouse_function
endif
	ret
init_lut:
;
; GSV start
;
	mov	dx, 3C4h	;EGA sequencer address
	mov	al, 7
	out	dx, al
	inc	dx
	in	al, dx
	and	al, 40h
	test	al, 40h
	push	es
	push	ds
	pop	es
	jnz	init_lut1
;
; EGA-style GSV card
;
	mov	dx, offset initial_color_table
	mov	ah,10h
	mov	al, 2
	int	10h
	jmps	init_lutend 
;
; VGA-style GSV card
;
init_lut1:
	mov	dx,offset initial_ident
	mov	ah,10h
	mov	al, 2
	int	10h		;Simple 1:1 mapping from GEM inks to VGA inks
;
; Load the VGA palette registers
;
	mov	dx,offset vga_col_table
	mov	bx, 0
	mov	cx, 10h
	mov	ax, 1012h
	int	10h		;Load 16 colours
	mov	ax, 1001h
	mov	bh, 0
	int	10h		;Set border to colour 0
	push	ax
	xor	ax, ax
	push	ds
	mov	ds, ax
	mov	bx, 488h	;?
	mov	al, [bx]
	pop	ds
	and	al, 30h
	cmp	al, 10h
	jnz	init_lut2
	push	dx
	push	ax
	mov	dx, 3D4h
	mov	al, 11h
	out	dx, al	;CRTC: Vertical retrace end
	inc	dx
	in	al, dx
	and	al, 7Fh
	out	dx, al
	dec	dx
	mov	al, 3
	out	dx, al	;CRTC: End horizontal blanking
	inc	dx
	in	al, dx
	add	al, 2
	out	dx, al
	pop	ax
	pop	dx
init_lut2:	
	pop	ax
init_lutend:
	pop	es
	ret
;
; GSV end
;
