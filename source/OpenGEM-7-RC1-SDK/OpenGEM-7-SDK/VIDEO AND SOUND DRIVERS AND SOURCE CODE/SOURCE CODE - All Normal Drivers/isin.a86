					; Isin+33j ...
		mov	ax, 18[bp]
		mov	bx, 0Ah
		cwd	
		idiv	bx
		mov	6[bp],	ax
		mov	ax, 18[bp]
		cwd	
		idiv	bx
		mov	ax, 6[bp]
		shl	ax, 1
		mov	si, ax
		mov	bx, sin_tbl[si]
		test	dx, dx
		mov	0[bp],	ax
		mov	8[bp],	dx
		mov	10[bp], bx
		jz	Isin11
		mov	ax, sin_tbl+2[si]
		sub	ax, bx
		imul	dx
		mov	cx, 0Ah
		cwd	
		idiv	cx
		add	10[bp], ax

Isin11:				; CODE XREF: Isin+8Ej
		cmp	word ptr 12[bp], 1
		jle	Isin12
		neg	word ptr 10[bp]

Isin12:				; CODE XREF: Isin+A5j
		mov	ax, 10[bp]
		add	sp, 0Eh
		pop	bp
		retn	



