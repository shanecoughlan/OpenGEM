					; gdp_rbox+D0p ...
		push	bp
		sub	sp, 0Eh
		mov	bp, sp

Isin1:				; CODE XREF: Isin+13j
		mov	ax, 18[bp]
		cmp	ax, 3600
		jle	Isin2
		sub	word ptr 18[bp], 3600
		jmps	Isin1
;컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴컴

