BOX_FILL:
;
; We ignore all the fancy word-alignment stuff. We will draw by bytes; this 
; may slow down drawing but the code becomes ridiculously simple.
;
		mov	bx, X1
		mov	ax, Y1
		call	concat
		if	VESA_MULTISEG
		 mov	ax, cs:VID_SEG
		else
		 mov	ax, graph_plane
		endif
		mov	es, ax
BOX_FILL_LINE:	mov	ax, NEXT_PAT
		or	ax, ax
		jz	BOX_FILL_MONO
		mov	ax, WRT_MODE	;XOR and INVTRAN behave like 1-bit ops
		cmp	ax, 2
		jnc	BOX_FILL_MONO	
		jmp	BOX_FILL_8BIT
;	
BOX_FILL_MONO:	mov	ax, 0Fh
		mov	dx,X1
		mov	bp,X2
		inc	bp
		sub	bp,dx
		mov	bx,dx
		and	bx,ax
		shl	bx,1
		mov	bx,bit_mask_table[bx]			
		push	bx		;BX = bit mask

		mov	si, Y1
		mov	cx, Y2
		sub	cx, si
		inc	cx		;CX = count of lines to paint
		and	si, patmsk
		mov	bx, patptr
		add	bx, si
		add	bx, si
		mov	patcnt, si
		mov	si, bx		;SI -> pattern
		mov	bx, WRT_MODE
		shl	bx, 1
		mov	ax, box_optbl[bx]
		pop	bx		;BX = bit mask
		call	ax
		retn	
;
BOX_FILL_8BIT:	mov	ax, 0Fh
		mov	dx, X1
		mov	bp, X2
		inc	bp
		sub	bp, dx		;BP = width
		mov	bx, dx		;BX = left edge

		mov	si, Y1		;SI = Y-coord
		mov	cx, Y2
		sub	cx, si
		inc	cx		;CX = count of lines to paint

		and	si, patmsk
		mov	bx, offset udpt_8bit
		push	si
		add	si, si	;*2
		add	si, si	;*4
		add	si, si	;*8
		add	si, si	;*16
		add	bx, si
		pop	si
		mov	patcnt, si
		mov	si, bx		;SI -> pattern
;
; DX = left edge
; SI-> pattern for row
; BP = width
; ES:DI -> destination pixel
;
box_tran_8bit:	push	di
		push	dx
		push	bp
		if	VESA_SPLITLINE
		 push	cs:VESA_SLICE
		endif

btr8loop:	mov	ax, dx
		and	ax, 0Fh
		push	si
		add	si, ax
		if VESA_SPLITLINE
		 lodsb	
		 call	stosb1024
		else
		 movsb		;Move byte from pattern to screen.
		endif
		pop	si
		inc	dx
		dec	bp
		jnz	btr8loop
		if	VESA_SPLITLINE
		 pop	cs:VESA_SLICE
		 call	setslice
		endif
		pop	bp
		pop	bx
		pop	di
;
; Next row, and SI needs recalcing.
;
		call	box_next8bit
		loop	box_tran_8bit
		ret

box_next8bit:	call	incdi1024
;
; Increment pattern pointer
;
		mov	ax,patcnt
		inc	ax
		and	ax,patmsk
		mov	patcnt,ax
		shl	ax,1	;*2	
		shl	ax,1	;*4	
		shl	ax,1	;*8
		shl	ax,1	;*16
		add	ax,offset udpt_8bit
		mov	si,ax
		ret


;ДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДДД

box_tran_mode:	
		if VESA_SPLITLINE
		 push	cs:VESA_SLICE
		endif
		push	di
		push	bx
		push	bp
box_tran_l:	mov	ax,[si]
		and	ax,bx
		jnz	box_tran_m
		if	VESA_SPLITLINE
		 call	bumpdi1024
		else
		 inc	di
		endif
		jmps	box_tran_n

box_tran_m:	mov	al, byte ptr FG_BP_1
		if	VESA_SPLITLINE
		 call	stosb1024
		else
		 stosb
		endif
box_tran_n:	ror	bx,1
		dec	bp
		jnz	box_tran_l	
		pop	bp
		pop	bx
		pop	di
		if VESA_SPLITLINE
		 pop	cs:VESA_SLICE
		 call	setslice
		endif
		call	box_nextrow
		loop	box_tran_mode
		ret
