;
; Enter with: ES:DI  -> first pixel
;             patcnt =  pattern index
;             si     =  pattern pointer
;             patmsk =  pattern mask
;             bp     =  width
;             cx     =  height 
;	      bx     =  pixel mask for pattern
;
box_replace_mode:
		if VESA_SPLITLINE
		 push	cs:VESA_SLICE
		endif
		push	di
		push	bx
		push	bp
box_replace_l:	mov	ax,[si]
		and	ax,bx
		mov	al,0
		jz	box_replace_m
		mov	al, byte ptr FG_BP_1
box_replace_m:	
		if	VESA_SPLITLINE
		 call	stosb1024
		else
		 stosb
		endif
		ror	bx,1
box_replace_n:	dec	bp
		jnz	box_replace_l	
		pop	bp
		pop	bx
		pop	di
		if VESA_SPLITLINE
		 pop	cs:VESA_SLICE
		 call	setslice
		endif
		call	box_nextrow
		loop	box_replace_mode
		ret
;
box_xor_mode:	
		if VESA_SPLITLINE
		 push	cs:VESA_SLICE
		endif
		push	di
		push	bx
		push	bp
box_xor_l:	mov	ax,[si]
		and	ax,bx
		mov	al,0
		jz	box_xor_m
;;;		mov	al, byte ptr FG_BP_1
		mov	al,0FFh
box_xor_m:	xor	es:[di],al
		if	VESA_SPLITLINE
		 call	bumpdi1024
		else
		 inc	di
		endif
		ror	bx,1
box_xor_n:	dec	bp
		jnz	box_xor_l	
		pop	bp
		pop	bx
		pop	di
		if VESA_SPLITLINE
		 pop	cs:VESA_SLICE
		 call	setslice
		endif
		call	box_nextrow
		loop	box_xor_mode
		ret
;
incdi1024:	add	di, bytes_line
		if	VESA_MULTISEG
		 if	WINDOW_SIZE ne 0
		  cmp	di, WINDOW_SIZE
		  jc	box_nxr1
		  sub	di, WINDOW_SIZE
		 else
		  jnc	box_nxr1	;Wrap at end of window
		 endif
		 inc	cs:VESA_SLICE
		 call	setslice
		endif
box_nxr1:	ret
;
		if	VESA_SPLITLINE
stosb1024:	mov	es:[di], al
bumpdi1024:	inc	di
		 if	WINDOW_SIZE ne 0
		  cmp	di, WINDOW_SIZE
		 endif
		jnz	box_nxr1
		inc	cs:VESA_SLICE
		call	setslice
		ret
		endif



box_nextrow:
		call	incdi1024
;
; Increment pattern pointer
;
		mov	ax,patcnt
		inc	ax
		and	ax,patmsk
		mov	patcnt,ax
		shl	ax,1	
		add	ax,patptr
		mov	si,ax
		ret


